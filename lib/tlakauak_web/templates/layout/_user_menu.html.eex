<ul>
<%= if @current_user do %>
  <li><%= @current_user.email %></li>
  <li><%= link "Perfil", to: Routes.user_settings_path(@conn, :edit) %></li>
  <li><%= link "Cerrar", to: Routes.user_session_path(@conn, :delete), method: :delete %></li>
<% else %>
  <li><%= link "Registrar", to: Routes.user_registration_path(@conn, :new) %></li>
  <li><%= link "Iniciar", to: Routes.user_session_path(@conn, :new) %></li>
<% end %>
</ul>
